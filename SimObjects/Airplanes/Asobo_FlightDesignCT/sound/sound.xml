	   <WwiseRTPC SimVar="PLANE ALT ABOVE GROUND" Units="FEET" Index="0" RTPCName="SIMVAR_PLANE_ALT_ABOVE_GROUND"/>
	   <Requires SimVar="PLANE ALT ABOVE GROUND" Units="FEET" Index="0">
			<Range UpperBound="200" />
		</Requires>
    </Sound>  
	
