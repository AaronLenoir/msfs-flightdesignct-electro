	</Sound>
	<Sound WwiseEvent="Combustion" CustomSuffix="Exhaust" ConeHeading="235" FadeOutType="2" FadeOutTime="0.2" RPMMin="2078" WwiseData="true" EngineIndex="1" ViewPoint="Outside">   
		<WwiseRTPC SimVar="GENERAL ENG COMBUSTION SOUND PERCENT" Units="PERCENT OVER 100" RTPCReleaseTime="0.5" Index="1" RTPCName="SIMVAR_GENERAL_ENG_COMBUSTION_SOUND_PERCENT" />
	</Sound>
